---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - posts
permalink: /tags/{{ tag }}/
---
<h1>Tagged "{{ tag }}"</h1>

<ol>
{% set taglist = collections[ tag ] %}
{% for post in taglist | reverse %}
<li>[{{post.url}}], [{{post.url | url}}]<a href="{{ post.url }}">{{ post.data.title }}</a></li>
{% endfor %}
</ol>
